{
  "compilerOptions": {
    "incremental": true,                              
    "composite": true,                              
    "tsBuildInfoFile": "./.tsbuildinfo",              
    "target": "es2016",                                 
    "module": "commonjs",                                
    "rootDir": "./",                               
    "allowJs": true,                                  
    "checkJs": true,                                 
    "outDir": "./dist",                                  
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true                            
  },
   "exclude": ["node_modules"],
}
